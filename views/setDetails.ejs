<% include partials/header.ejs %>
    <div class="container">
        <div class="row">
            <h2 class="setName"><%= cards[0].editions[0].set %></h2>
            <% for(var i = 0; i < cards.length; i++){ %>
                <% if(cards[i].editions[0].multiverse_id){ %>
                    <div class="col-md-4 col-sm-6 col-xs-12">
                        <div class="thumbnail">
                            <div class="titleAndSet">
                                <p><h3><%= cards[i].name %></h3></p>
                            </div>
                            <p><img class="thumbImg" src="<%= cards[i].editions[0].image_url %>"></p>
                            <p><a href="/<%= cards[i].editions[0].multiverse_id %>/details">Learn more</a></p>
                        </div>
                    </div>
                <% } %>
            <% }; %>
        </div>
    </div>
<% include partials/footer.ejs %>